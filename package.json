{
  "name": "functional-helpers",
  "description": "General usage utility functions.",
  "version": "0.0.0",
  "main": "dist/main.min.js",
  "author": "Joshua Heagle",
  "license": "MIT",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "htmldocs": "jsdoc -R MAIN.md -r src/ -d docs",
    "readme": "cp MAIN.md README.md && echo \"\" >> README.md && jsdoc2md src/main.js src/helpers/*.js src/helpers/arrays/*.js src/helpers/descriptors/samples/*.js src/helpers/descriptors/*.js src/helpers/functions/*.js src/helpers/numbers/*.js src/helpers/objects/*.js src/helpers/strings/*.js >> README.md",
    "resolutions": "npx npm-force-resolutions",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "ansi-regex": "^5.0.1",
    "babel-jest": "^27.5.1",
    "browserify": "^17.0.0",
    "del": "^6.0.0",
    "glob-parent": "^6.0.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-standard": "^14.0.0",
    "gulp-uglify-es": "^3.0.0",
    "jest": "^27.5.1",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.1",
    "standard": "^16.0.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.17.2",
    "core-js": "^3.21.1",
    "npm": "^8.5.2",
    "regenerator-runtime": "^0.13.9"
  },
  "directories": {
    "test": "tests"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "glob-parent": "6.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jheagle/functional-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/jheagle/functional-helpers/issues"
  },
  "homepage": "https://github.com/jheagle/functional-helpers#readme",
  "standard": {
    "env": [
      "jest"
    ]
  },
  "files": [
    "dist",
    "browser"
  ],
  "jest": {
    "testMatch": ["<rootDir>/src/**/*.test.js"]
  }
}
